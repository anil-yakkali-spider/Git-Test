		<section name="databaseSettings" type="System.Configuration.NameValueSectionHandler" />
		<sectionGroup name="spring">
			<section name="context" type="dispatchOfficeWeb.MvcAppCode.Context.Impl.MvcWebContextHandler, dispatchOfficeWeb" />
			<section name="parsers" type="Spring.Context.Support.NamespaceParsersSectionHandler, Spring.Core" />
		</sectionGroup>
		<section name="dataConfiguration" type="Microsoft.Practices.EnterpriseLibrary.Data.Configuration.DatabaseSettings, Microsoft.Practices.EnterpriseLibrary.Data, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
		<sectionGroup name="telerik.web.ui">
			<section name="radCompression" type="Telerik.Web.UI.RadCompressionConfigurationSection, Telerik.Web.UI, PublicKeyToken=121FAE78165BA3D4" allowDefinition="MachineToApplication" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<syscache>
		<cache region="LongTermReadOnly" expiration="86400" priority="5" />
		<cache region="LongTermReadWrite" expiration="43200" priority="6" />
		<cache region="ShortTermReadWrite" expiration="3600" priority="4" />
		<cache region="VeryShortTermReadWrite" expiration="120" priority="3" />
		<cache region="UltraShortTermReadOnly" expiration="15" priority="5" />
	</syscache>
	<log4net>
		<appender name="Console" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%d [%t]: %m%n" />
			</layout>
		</appender>
		<appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
			<param name="File" value="logs\dispatchOfficeWeb.log" />
			<param name="AppendToFile" value="true" />
			<param name="RollingStyle" value="Composite" />
			<param name="MaximumFileSize" value="50MB" />
			<param name="MaxSizeRollBackups" value="31" />
			<param name="StaticLogFileName" value="true" />
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="%d %-5level [%c - (%t)]: %m%n" />
			</layout>
		</appender>
		<appender name="DB" type="log4net.Appender.AdoNetAppender">
			<bufferSize value="3" />
			<reconnectOnError value="true" />
			<connectionType value="System.Data.SqlClient.SqlConnection, System.Data, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" />
			<commandText value="EXEC WriteLog @module_pkey=5, @Severity=@log_severity, @MachineName=@hostname, @ProcessID=@pid, @ProcessName=@proc_name, @ThreadName=@thread, @Message=@msg;">
			</commandText>
			<parameter>
				<parameterName value="@log_severity" />
				<dbType value="String" />
				<size value="20" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%level" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@hostname" />
				<dbType value="String" />
				<size value="32" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{log4net:HostName}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@pid" />
				<dbType value="Int32" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{pid}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@proc_name" />
				<dbType value="String" />
				<size value="512" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%property{proc_name}" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@thread" />
				<dbType value="String" />
				<size value="512" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%thread" />
				</layout>
			</parameter>
			<parameter>
				<parameterName value="@msg" />
				<dbType value="String" />
				<size value="1500" />
				<layout type="log4net.Layout.PatternLayout">
					<conversionPattern value="%message" />
				</layout>
			</parameter>
		</appender>
		<!-- This section controls the NHibernate Logging -->
		<logger name="NHibernate">
			<level value="WARN" />
		</logger>
		<logger name="NHibernate.SQL">
			<level value="WARN" />
		</logger>
		<root>
			<level value="WARN" />
			<!-- Should typically be 'WARN' -->
			<appender-ref ref="Console" />
			<appender-ref ref="RollingFile" />
			<appender-ref ref="DB" />
		</root>
	</log4net> 
